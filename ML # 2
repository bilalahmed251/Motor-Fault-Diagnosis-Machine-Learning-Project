csv\"), recursive=True)\n","    files += glob.glob(os.path.join(DATA_PATH, \"**\", \"*.txt\"), recursive=True)\n","\n","    if not files:\n","        print(\"\\nCRITICAL ERROR: No files found!\")\n","        print(f\"I checked this folder: {DATA_PATH}\")\n","        return\n","\n","    print(f\"Found {len(files)} files. Processing...\")\n","\n","    for filepath in files:\n","        filename = filepath.lower()\n","\n","        label = -1\n","        if \"healthy\" in filename or \"normal\" in filename: label = 0\n","        elif \"brb\" in filename or \"broken\" in filename: label = 13\n","        elif \"inner\" in filename or \"i-r\" in filename:\n","            if \"0.7\" in filename: label = 1\n","            elif \"0.9\" in filename: label = 2\n","            elif \"1.1\" in filename: label = 3\n","            elif \"1.3\" in filename: label = 4\n","            elif \"1.5\" in filename: label = 5\n","            elif \"1.7\" in filename: label = 6\n","        elif \"outer\" in filename or \"o-r\" in filename:\n","            if \"0.7\" in filename: label = 7\n","            elif \"0.9\" in filename: label = 8\n","            elif \"1.1\" in filename: label = 9\n","            elif \"1.3\" in filename: label = 10\n","            elif \"1.5\" in filename: label = 11\n","            elif \"1.7\" in filename: label = 12\n","\n","        if label == -1: continue\n","\n","        try:\n","            try:\n","                df_temp = pd.read_csv(filepath, header=None, engine='python')\n","            except:\n","                df_temp = pd.read_csv(filepath, header=None, delim_whitespace=True)\n","\n","            signal_data = df_temp.values.flatten()\n","\n","            signal_data = pd.to_numeric(signal_data, errors='coerce')\n","            signal_data = signal_data[~np.isnan(signal_data)]\n","\n","            if len(signal_data) >= WINDOW_SIZE:\n","                num_chunks = len(signal_data) // WINDOW_SIZE\n","                for i in range(num_chunks):\n","                    start = i * WINDOW_SIZE\n","                    end = start + WINDOW_SIZE\n","                    chunk = signal_data[start:end]\n","                    data_rows.append(chunk)\n","                    labels.append(label)\n","\n","        except Exception as e:\n","            print(f\"Error reading {filepath}: {e}\")\n","\n","    if len(data_rows) > 0:\n","        cols = [f\"f{i}\" for i in range(WINDOW_SIZE)]\n","        df_final = pd.DataFrame(data_rows, columns=cols)\n","        df_final['label'] = labels\n","\n","        output_filename = \"processed_motor_data.csv\"\n","        df_final.to_csv(output_filename, index=False)\n","        print(\"\\n\" + \"=\"*40)\n","        print(f\"SUCCESS! '{output_filename}' created.\")\n","        print(f\"Total samples extracted: {len(df_final)}\")\n","        print(\"=\"*40)\n","    else:\n","        print(\"\\nFAILURE: Files were found, but no valid data rows could be extracted.\")\n","\n","generate_csv_dataset()\n","\n","print(\"Loading dataset...\")\n","try:\n","    df = pd.read_csv(\"processed_motor_data.csv\")\n","except:\n","    print(\"Error: Could not find 'processed_motor_data.csv'.\")\n","    exit()\n","\n","print(\"\\n\" + \"=\"*40)\n","print(\" QUESTION 1: BINARY LOGISTIC REGRESSION\")\n","print(\"=\"*40)\n","\n","X = df.drop('label', axis=1)\n","y_original = df['label']\n","y_binary = y_original.apply(lambda x: 0 if x == 0 else 1)\n","\n","X_train, X_test, y_train, y_test = train_test_split(X, y_binary, test_size=0.2, random_state=42)\n","\n","model = LogisticRegression(max_iter=1000)\n","model.fit(X_train, y_train)\n","y_pred = model.predict(X_test)\n","y_prob = model.predict_proba(X_test)[:, 1]\n","\n","tn, fp, fn, tp = confusion_matrix(y_test, y_pred).ravel()\n","accuracy = accuracy_score(y_test, y_pred)\n","precision = precision_score(y_test, y_pred)\n","recall = recall_score(y_test, y_pred)\n","f1 = f1_score(y_test, y_pred)\n","specificity = tn / (tn + fp) if (tn + fp) > 0 else 0\n","\n","print(f\"Accuracy:    {accuracy*100:.2f}%\")\n","print(f\"Precision:   {precision*100:.2f}%\")\n","print(f\"Sensitivity: {recall*100:.2f}% (Recall)\")\n","print(f\"Specificity: {specificity*100:.2f}%\")\n","print(f\"F1 Score:    {f1*100:.2f}%\")\n","\n","plt.figure(figsize=(6, 5))\n","cm_binary = confusion_matrix(y_test, y_pred)\n","sns.heatmap(cm_binary, annot=True, fmt='d', cmap='Blues', xticklabels=['Healthy', 'Unhealthy'], yticklabels=['Healthy', 'Unhealthy'])\n","plt.title('Graph 1: Binary Classification Confusion Matrix')\n","plt.ylabel('Actual')\n","plt.xlabel('Predicted')\n","plt.show()\n","\n","fpr, tpr, thresholds = roc_curve(y_test, y_prob)\n","roc_auc = auc(fpr, tpr)\n","\n","plt.figure(figsize=(6, 5))\n","plt.plot(fpr, tpr, color='darkorange', lw=2, label=f'ROC curve (area = {roc_auc:.2f})')\n","plt.plot([0, 1], [0, 1], color='navy', lw=2, linestyle='--')\n","plt.xlim([0.0, 1.0])\n","plt.ylim([0.0, 1.05])\n","plt.xlabel('False Positive Rate')\n","plt.ylabel('True Positive Rate')\n","plt.title('Graph 2: ROC Curve (Binary)')\n","plt.legend(loc=\"lower right\")\n","plt.grid(True)\n","plt.show()\n","\n","print(\"\\n\" + \"=\"*40)\n","print(\" QUESTION 2: MULTICLASS LOGISTIC REGRESSION\")\n","print(\"=\"*40)\n","\n","X_train_m, X_test_m, y_train_m, y_test_m = train_test_split(X, y_original, test_size=0.2, random_state=42)\n","\n","model_multi = LogisticRegression(max_iter=1000, multi_class='multinomial', solver='lbfgs')\n","model_multi.fit(X_train_m, y_train_m)\n","y_pred_m = model_multi.predict(X_test_m)\n","\n","print(f\"Overall Accuracy: {accuracy_score(y_test_m, y_pred_m)*100:.2f}%\")\n","print(\"\\nDetailed Report per Class:\")\n","report_dict = classification_report(y_test_m, y_pred_m, output_dict=True)\n","print(classification_report(y_test_m, y_pred_m))\n","\n","plt.figure(figsize=(10, 8))\n","cm_multi = confusion_matrix(y_test_m, y_pred_m)\n","sns.heatmap(cm_multi, annot=True, fmt='d', cmap='viridis')\n","plt.title('Graph 3: Multiclass Confusion Matrix (14 Classes)')\n","plt.ylabel('Actual Class')\n","plt.xlabel('Predicted Class')\n","plt.show()\n","\n","classes = []\n","precisions = []\n","recalls = []\n","\n","for key, value in report_dict.items():\n","    if key.isdigit():\n","        classes.append(key)\n","        precisions.append(value['precision'])\n","        recalls.append(value['recall'])\n","\n","x = np.arange(len(classes))\n","width = 0.35\n","\n","plt.figure(figsize=(12, 6))\n","plt.bar(x - width/2, precisions, width, label='Precision', color='skyblue')\n","plt.bar(x + width/2, recalls, width, label='Recall', color='salmon')\n","plt.xlabel('Class Label (0-13)')\n","plt.ylabel('Score (0-1)')\n","plt.title('Graph 4: Precision vs Recall per Class')\n","plt.xticks(x, classes)\n","plt.legend()\n","plt.grid(axis='y', linestyle='--', alpha=0.7)\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"3wawaa9cPYYz","outputId":"68de9859-19c0-4e0c-a66a-0bb5f5dc190a"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Drive already mounted at /content/drive; to attempt to forcibly remount, call drive.mount(\"/content/drive\", force_remount=True).\n","Looking for files in: /content/drive/MyDrive/ColabNotebook ...\n","Found 41 files. Processing...\n","\n","========================================\n","SUCCESS! 'processed_motor_data.csv' created.\n","Total samples extracted: 15538\n","========================================\n","Loading dataset...\n","\n","========================================\n"," QUESTION 1: BINARY LOGISTIC REGRESSION\n","========================================\n","Accuracy:    98.07%\n","Precision:   98.07%\n","Sensitivity: 100.00% (Recall)\n","Specificity: 0.00%\n","F1 Score:    99.03%\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 600x500 with 2 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 600x500 with 1 Axes>"],"image/png":"\